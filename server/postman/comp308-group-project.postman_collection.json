							" method: 'POST',\r",
							" header: {\r",
							"    'Content-Type': 'application/json'\r",
							" },\r",
							"body: {\r",
							"   mode: 'raw',\r",
							"   raw: JSON.stringify({ \"username\": pm.environment.get(\"username\"), \"password\" : pm.environment.get(\"password\")})\r",
							"}\r",
							"}, function (err, res) {\r",
							"    console.log(res.json().token)\r",
							"    pm.environment.set(\"token\", res.json().token);\r",
							"});"
